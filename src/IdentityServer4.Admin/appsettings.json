{
  "Version": "20190306",
  "SiteName": "IDS4 Admin",
  "DatabaseProvider": "sqlserver",
  "ConnectionString": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=IdentityServer;Integrated Security=True;Connect Timeout=30;Encrypt=False;TrustServerCertificate=False;ApplicationIntent=ReadWrite;MultiSubnetFailover=False",
  "OpenRegister": true,
  "StorageRoot": "wwwroot",
  "RequireUppercase": false,
  "RequireNonAlphanumeric": false,
  "RequireDigit": false,
  "RequiredLength": 6,
  "RequireUniqueEmail": false,
  "EnableTokenCleanup": true,
  "AllowAnonymousUserQuery": true,
  "AllowLocalLogin": true,
  "AllowRememberLogin": true,
  "RememberMeLoginDuration": 30,
  "ShowLogoutPrompt": true,
  "AutomaticRedirectAfterSignOut": true,
  "WindowsAuthenticationSchemeName": "Windows",
  "IncludeWindowsGroups": false,
  "InvalidCredentialsErrorMessage": "用户名或密码错误",
  "EnableOfflineAccess": true,
  "OfflineAccessDisplayName": "脱机访问",
  "OfflineAccessDescription": "允许在授权服务脱机的情况下访问您的应用、资源",
  "MustChooseOneErrorMessage": "至少需要选择一个权限",
  "InvalidSelectionErrorMessage": "Invalid selection",
  "AllowedHosts": "*"
}
